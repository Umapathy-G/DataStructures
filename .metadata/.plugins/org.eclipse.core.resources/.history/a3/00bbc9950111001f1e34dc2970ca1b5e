package com.example._Basic;

public class Main {

	public void selectionSort(int[] arr) {
		for (int i = 0; i < arr.length - 1; i++) {
			int min = i;
			for (int j = i + 1; j < arr.length; j++)
				if (arr[min] > arr[j])
					min = j;
		}
	}

	public static void main(String[] args) {
		int[] numbers = { 1, 2, 3, 4, 5 };

	}

}
